      "source": [
        "# Lógica de programação\n",
        "\n",
        "# Passo 0 - Entender o desafio que você quer resolver\n",
        "\n",
        "# Passo 1 - Percorrer todos os aquivos da pasta base de dados (Pasta Vendas)\n",
        "import os\n",
        "import pandas as pd\n",
        "import plotly.express as px\n",
        "\n",
        "lista_arquivo = os.listdir(\"/content/drive/MyDrive/Conhecimento é Poder/Curso basico de Python/Vendas\")\n",
        "display(lista_arquivo)\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "tabela_total = pd.DataFrame()\n",
        "\n",
        "# Passo 2 - Importar as bases de dados de vendas\n",
        "for arquivo in lista_arquivo:\n",
        "  # se tem \"Vendas\" no nome do arquivo, então\n",
        "\n",
        "  if \"Vendas\" in arquivo:\n",
        "\n",
        "    # importar o arquivo\n",
        "    tabela = pd.read_csv(f\"/content/drive/MyDrive/Conhecimento é Poder/Curso basico de Python/Vendas/{arquivo}\")\n",
        "    tabela_total = tabela_total.append(tabela)\n",
        "\n"
      ],
      "metadata": {
        "id": "a7J6H7ssHZ5u"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Passo 3 - Tratar / Compilar as bases de dados\n",
        "display(tabela_total)\n",
        "\n"
      ],
      "metadata": {
        "id": "MyNEoIf-XDYE"
      },
      "execution_count": null,
      "outputs": []
    },
    },
    {
      "cell_type": "code",
      "source": [
        "# Passo 4 - Calcular o produto mais vendido (em quantidade)\n",
        "tabela_produtos = tabela_total.groupby('Produto').sum()\n",
        "tabela_produtos = tabela_produtos[[\"Quantidade Vendida\"]].sort_values(by=\"Quantidade Vendida\", ascending=False)\n",
        "display(tabela_produtos)\n"
      ],
      "metadata": {
        "id": "qFbY4nnMXJej"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Passo 5 - Calcular o produto que mais faturou (em faturamento)\n",
        "tabela_total['Faturamento'] = tabela_total['Quantidade Vendida'] * tabela_total['Preco Unitario']\n",
        "\n",
        "tabela_faturamento = tabela_total.groupby('Produto').sum()\n",
        "tabela_faturamento = tabela_faturamento[[\"Faturamento\"]].sort_values(by=\"Faturamento\", ascending=False)\n",
        "display(tabela_faturamento)"
      ],
      "metadata": {
        "id": "WZ_ZYddwXK6U"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Passo 6 - Calcular a loja / cidade que mais vendeu (em faturamento - Criar um grafico / dashboard)\n",
        "tabela_lojas = tabela_total.groupby('Loja').sum()\n",
        "tabela_lojas = tabela_lojas[['Faturamento']]\n",
        "display(tabela_lojas)"
      ],
      "metadata": {
        "id": "Ew8jwLdaXMiU"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "grafico = px.bar(tabela_lojas, x=tabela_lojas.index, y='Faturamento')\n",
        "grafico.show()\n",
        "\n"
      ],
